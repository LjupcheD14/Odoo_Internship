<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_sale_order_list_upload_inherit_urgent" model="ir.ui.view">
        <field name="name">sale.order.list.upload.inherit.urgent</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.sale_order_list_upload"/>
        <field name="arch" type="xml">
            <xpath expr="//field" position="before">
                <field name="is_urgent" widget="boolean" string="Urgent"/>
            </xpath>
        </field>
    </record>
</odoo>

